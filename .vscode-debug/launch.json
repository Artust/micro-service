  "version": "0.2.0",
  "configurations": [
    {
      "name": "Corporation",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/services/corporation/cmd",
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "name": "Center",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/services/center/cmd",
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "name": "POS",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/services/pos/cmd",
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "name": "Talk Session",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/services/talk_session/cmd",
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "name": "Streaming",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/services/streaming/cmd",
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "name": "Gateway",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/services/gateway/cmd",
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "name": "Upload",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/services/upload/cmd",
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "name": "Account Management",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/services/account_management/cmd",
      "envFile": "${workspaceFolder}/.env"
    }
  ],
  "compounds": [
    {
      "name": "All",
      "configurations": [
        "Corporation",
        "Center",
        "POS",
        "Talk Session",
        "Streaming",
        "Upload",
        "Account Management",
        "Gateway"
      ],
      "stopAll": true
    }
  ]
}
